{
  "kind": "collectionType",
  "collectionName": "contact_submissions",
  "info": {
    "singularName": "contact-submission",
    "pluralName": "contact-submissions",
    "displayName": "Contact Submission",
    "description": "Contact form submissions from visitors"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "subject": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "ipAddress": {
      "type": "string",
      "private": true
    },
    "userAgent": {
      "type": "text",
      "private": true
    },
    "isRead": {
      "type": "boolean",
      "default": false
    },
    "response": {
      "type": "text",
      "description": "Admin response to the contact submission"
    }
  }
} 